layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>gyst</title>
 <link href="http://mattscilipoti.github.com/gyst/atom.xml" rel="self"/>
 <link href="http://mattscilipoti.github.com/gyst/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://mattscilipoti.github.com/</id>
 <author>
   <name>Matt Scilipoti</name>
   <email>matt+gyst@scilipoti.name</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://mattscilipoti.github.com/gyst{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://mattscilipoti.github.com/gyst{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>

